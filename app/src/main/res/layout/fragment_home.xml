                            android:layout_width="0dp"
                            android:layout_height="4dp"
                            android:layout_weight="1"
                            android:layout_gravity="center_vertical"
                            android:background="@color/darkGray"
                            />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:weightSum="100"
                        android:layout_marginTop="8dp"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/home_txt_excerciseyest"
                            android:layout_width="0dp"
                            android:layout_weight="55"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            style="@style/ContentTitle"
                            android:textColor="@color/darkBlue"
                            android:text="@string/str_yesterday"/>
                        <ImageView android:id="@+id/home_excercise_yest_iv_onestar"
                            android:layout_width="0dp"
                            android:layout_height="20dp"
                            android:layout_weight="15"
                            android:layout_gravity="center_vertical"
                            android:src="@drawable/icon_onestar_d"
                            />
                        <ImageView android:id="@+id/home_excercise_yest_iv_twostar"
                            android:layout_width="0dp"
                            android:layout_height="20dp"
                            android:layout_weight="15"
                            android:layout_gravity="center_vertical"
                            android:src="@drawable/icon_twostar_d"
                            />
                        <ImageView android:id="@+id/home_excercise_yest_iv_tripstar"
                            android:layout_width="0dp"
                            android:layout_height="30dp"
                            android:layout_weight="15"
                            android:layout_gravity="center_vertical"
                            android:src="@drawable/icon_tripstar_d"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="8dp"
                        android:background="@color/white"
                        android:weightSum="1"
                        android:layout_marginRight="8dp"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/home_excercise_yest_progress"
                            android:layout_width="0dp"
                            android:layout_height="8dp"
                            android:layout_weight="0.5"
                            android:layout_gravity="center_vertical"
                            android:background="@color/orange"
                            />
                        <TextView
                            android:id="@+id/home_excercise_yest_progressbg"
                            android:layout_width="0dp"
                            android:layout_height="4dp"
                            android:layout_weight="1"
                            android:layout_gravity="center_vertical"
                            android:background="@color/darkGray"
                            />
                    </LinearLayout>
                </LinearLayout>
                <TextView
                    android:layout_below="@id/home_linear_content_excercise"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/darkGray"
                    />
            </RelativeLayout>
            <!-- End Excercise-->
            <!-- Start Vitamins-->
            <RelativeLayout
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/home_txt_vitamins"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    style="@style/SectionTitle"
                    android:text="@string/mt_vitamins"/>
                <TextView
                    android:layout_below="@id/home_txt_vitamins"
                    android:layout_marginTop="8dp"
                    android:id="@+id/home_txt_line6"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/darkGray"
                    />
                <LinearLayout android:id="@+id/home_linear_content_vitamins"
                    android:layout_below="@id/home_txt_line6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:paddingLeft="20dp"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    android:orientation="horizontal">
                    <!-- Today & Yesterday title -->
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/home_txt_vitaminstoday"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            style="@style/ContentTitle"
                            android:text="@string/str_today"/>
                        <TextView
                            android:id="@+id/home_txt_vitaminsyest"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            style="@style/ContentTitle"
                            android:textColor="@color/darkBlue"
                            android:text="@string/str_yesterday"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="10dp"
                        android:background="@color/white"
                        android:weightSum="2"
                        android:orientation="vertical">
                        <!-- Today Stars -->
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/white"
                            android:layout_weight="1"
                            android:weightSum="10"
                            android:orientation="horizontal">
                            <ImageView android:id="@+id/home_vitamins_today_iv_1"
                                android:layout_width="0dp"
                                android:layout_height="20dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_onestar_d"
                                />
                        </LinearLayout>
                        <!-- Yesterday stars -->
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/white"
                            android:layout_weight="1"
                            android:layout_marginTop="8dp"
                            android:weightSum="10"
                            android:orientation="horizontal">
                            <ImageView android:id="@+id/home_vitamins_yest_iv_1"
                                android:layout_width="0dp"
                                android:layout_height="20dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_onestar_d"
                                />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <TextView
                    android:layout_below="@id/home_linear_content_vitamins"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/darkGray"
                    />
            </RelativeLayout>
            <!-- End Vitamins-->
        </LinearLayout>
    </ScrollView>


</RelativeLayout>
