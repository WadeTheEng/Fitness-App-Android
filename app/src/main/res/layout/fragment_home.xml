                            android:layout_height="20dp"
                            android:layout_weight="15"
                            android:layout_gravity="center_vertical"
                            android:src="@drawable/icon_twostar_d"
                            />
                        <ImageView android:id="@+id/home_prot_today_iv_tripstar"
                            android:layout_width="0dp"
                            android:layout_height="30dp"
                            android:layout_weight="15"
                            android:layout_gravity="center_vertical"
                            android:src="@drawable/icon_tripstar_d"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="8dp"
                        android:background="@color/white"
                        android:weightSum="1"
                        android:layout_marginRight="8dp"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/home_prot_today_progress"
                            android:layout_width="0dp"
                            android:layout_height="8dp"
                            android:layout_weight="0.1"
                            android:layout_gravity="center_vertical"
                            android:background="@color/orange"
                            />
                        <TextView
                            android:id="@+id/home_prot_today_progressbg"
                            android:layout_width="0dp"
                            android:layout_height="4dp"
                            android:layout_weight="1"
                            android:layout_gravity="center_vertical"
                            android:background="@color/darkGray"
                            />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:weightSum="100"
                        android:layout_marginTop="8dp"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/home_txt_protyest"
                            android:layout_width="0dp"
                            android:layout_weight="55"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            style="@style/ContentTitle"
                            android:textColor="@color/darkBlue"
                            android:text="@string/str_yesterday"/>
                        <ImageView android:id="@+id/home_prot_yest_iv_onestar"
                            android:layout_width="0dp"
                            android:layout_height="20dp"
                            android:layout_weight="15"
                            android:layout_gravity="center_vertical"
                            android:src="@drawable/icon_onestar_d"
                            />
                        <ImageView android:id="@+id/home_prot_yest_iv_twostar"
                            android:layout_width="0dp"
                            android:layout_height="20dp"
                            android:layout_weight="15"
                            android:layout_gravity="center_vertical"
                            android:src="@drawable/icon_twostar_d"
                            />
                        <ImageView android:id="@+id/home_prot_yest_iv_tripstar"
                            android:layout_width="0dp"
                            android:layout_height="30dp"
                            android:layout_weight="15"
                            android:layout_gravity="center_vertical"
                            android:src="@drawable/icon_tripstar_d"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="8dp"
                        android:background="@color/white"
                        android:weightSum="1"
                        android:layout_marginRight="8dp"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/home_prot_yest_progress"
                            android:layout_width="0dp"
                            android:layout_height="8dp"
                            android:layout_weight="0.5"
                            android:layout_gravity="center_vertical"
                            android:background="@color/orange"
                            />
                        <TextView
                            android:id="@+id/home_prot_yest_progressbg"
                            android:layout_width="0dp"
                            android:layout_height="4dp"
                            android:layout_weight="1"
                            android:layout_gravity="center_vertical"
                            android:background="@color/darkGray"
                            />
                    </LinearLayout>
                </LinearLayout>
                <TextView
                    android:layout_below="@id/home_linear_content_protein"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/darkGray"
                    />
            </RelativeLayout>
            <!-- End Protein-->
            <!-- Start Water-->
            <RelativeLayout
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/home_txt_water"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    style="@style/SectionTitle"
                    android:text="@string/mt_water"/>
                <TextView
                    android:layout_below="@id/home_txt_water"
                    android:layout_marginTop="8dp"
                    android:id="@+id/home_txt_line4"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/darkGray"
                    />
                <LinearLayout android:id="@+id/home_linear_content_water"
                    android:layout_below="@id/home_txt_line4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:paddingLeft="20dp"
                    android:paddingTop="5dp"
                    android:paddingBottom="10dp"
                    android:orientation="vertical">
                    <!-- Today & Yesterday title -->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:orientation="horizontal"
                android:weightSum="100">
                <TextView
                    android:id="@+id/home_txt_watertoday"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    style="@style/ContentTitle"
                    android:layout_gravity="center_vertical"
                    android:text="@string/str_today"
                    android:layout_weight="55"
                    />
                <ImageView android:id="@+id/home_water_today_iv_onestar"
                    android:layout_width="0dp"
                    android:layout_height="20dp"
                    android:layout_weight="15"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/icon_onestar_d"
                    />
                <ImageView android:id="@+id/home_water_today_iv_twostar"
                    android:layout_width="0dp"
                    android:layout_height="20dp"
                    android:layout_weight="15"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/icon_twostar_d"
                    />
                <ImageView android:id="@+id/home_water_today_iv_tripstar"
                    android:layout_width="0dp"
                    android:layout_height="30dp"
                    android:layout_weight="15"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/icon_tripstar_d"/>
            </LinearLayout>

             <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="8dp"
                android:background="@color/white"
                android:weightSum="1"
                android:layout_marginRight="8dp"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/home_water_today_progress"
                    android:layout_width="0dp"
                    android:layout_height="8dp"
                    android:layout_weight="0.1"
                    android:layout_gravity="center_vertical"
                    android:background="@color/orange"
                    />
                <TextView
                    android:id="@+id/home_water_today_progressbg"
                    android:layout_width="0dp"
                    android:layout_height="4dp"
                    android:layout_weight="1"
                    android:layout_gravity="center_vertical"
                    android:background="@color/darkGray"
                    />
               </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:weightSum="100"
                    android:layout_marginTop="8dp"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/home_txt_wateryest"
                        android:layout_width="0dp"
                        android:layout_weight="55"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        style="@style/ContentTitle"
                        android:textColor="@color/darkBlue"
                        android:text="@string/str_yesterday"/>
                    <ImageView android:id="@+id/home_water_yest_iv_onestar"
                        android:layout_width="0dp"
                        android:layout_height="20dp"
                        android:layout_weight="15"
                        android:layout_gravity="center_vertical"
                        android:src="@drawable/icon_onestar_d"
                        />
                    <ImageView android:id="@+id/home_water_yest_iv_twostar"
                        android:layout_width="0dp"
                        android:layout_height="20dp"
                        android:layout_weight="15"
                        android:layout_gravity="center_vertical"
                        android:src="@drawable/icon_twostar_d"
                        />
                    <ImageView android:id="@+id/home_water_yest_iv_tripstar"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_weight="15"
                        android:layout_gravity="center_vertical"
                        android:src="@drawable/icon_tripstar_d"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="8dp"
                    android:background="@color/white"
                    android:weightSum="1"
                    android:layout_marginRight="8dp"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/home_water_yest_progress"
                        android:layout_width="0dp"
                        android:layout_height="8dp"
                        android:layout_weight="0.5"
                        android:layout_gravity="center_vertical"
                        android:background="@color/orange"
                        />
                    <TextView
                        android:id="@+id/home_water_yest_progressbg"
                        android:layout_width="0dp"
                        android:layout_height="4dp"
                        android:layout_weight="1"
                        android:layout_gravity="center_vertical"
                        android:background="@color/darkGray"
                        />
                </LinearLayout>
              </LinearLayout>
                <TextView
                    android:layout_below="@id/home_linear_content_water"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/darkGray"
                    />
            </RelativeLayout>
            <!-- End Water-->
            <!-- Start Excercise-->
            <RelativeLayout
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/home_txt_excercise"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    style="@style/SectionTitle"
                    android:text="@string/mt_exercise"/>
                <TextView
                    android:layout_below="@id/home_txt_excercise"
                    android:layout_marginTop="8dp"
                    android:id="@+id/home_txt_line5"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/darkGray"
                    />
                <LinearLayout android:id="@+id/home_linear_content_excercise"
                    android:layout_below="@id/home_txt_line5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:paddingLeft="20dp"
                    android:paddingTop="5dp"
                    android:paddingBottom="10dp"
                    android:orientation="vertical">
                    <!-- Today & Yesterday title -->
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:orientation="horizontal"
                        android:weightSum="100">
                        <TextView
                            android:id="@+id/home_txt_excercisetoday"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            style="@style/ContentTitle"
                            android:layout_gravity="center_vertical"
                            android:text="@string/str_today"
                            android:layout_weight="55"
                            />
                        <ImageView android:id="@+id/home_excercise_today_iv_onestar"
                            android:layout_width="0dp"
                            android:layout_height="20dp"
                            android:layout_weight="15"
                            android:layout_gravity="center_vertical"
                            android:src="@drawable/icon_onestar_d"
                            />
                        <ImageView android:id="@+id/home_excercise_today_iv_twostar"
                            android:layout_width="0dp"
                            android:layout_height="20dp"
                            android:layout_weight="15"
                            android:layout_gravity="center_vertical"
                            android:src="@drawable/icon_twostar_d"
                            />
                        <ImageView android:id="@+id/home_excercise_today_iv_tripstar"
