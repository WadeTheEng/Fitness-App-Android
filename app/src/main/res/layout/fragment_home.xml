<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@color/bgGray"
    tools:context=".HomeFragment">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <!-- start Summary -->
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/home_txt_summary"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    style="@style/SectionTitle"
                    android:text="@string/str_summary"/>
                <TextView
                    android:layout_below="@id/home_txt_summary"
                    android:layout_marginTop="8dp"
                    android:id="@+id/home_txt_line1"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/darkGray"
                    />
                <LinearLayout android:id="@+id/home_linear_content"
                    android:layout_below="@id/home_txt_line1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:paddingLeft="20dp"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    android:orientation="horizontal">
                    <!-- Today & Yesterday title -->
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/home_txt_sumtoday"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            style="@style/ContentTitle"
                            android:text="@string/str_today"/>
                        <TextView
                            android:id="@+id/home_txt_sumyest"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            style="@style/ContentTitle"
                            android:textColor="@color/darkBlue"
                            android:text="@string/str_yesterday"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="10dp"
                        android:background="@color/white"
                        android:weightSum="2"
                        android:orientation="vertical">
                        <!-- Today Stars -->
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/white"
                            android:layout_weight="1"
                            android:weightSum="10"
                            android:orientation="horizontal">
                            <ImageView android:id="@+id/home_today_iv_1"
                                android:layout_width="0dp"
                                android:layout_height="20dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_onestar_d"
                                />
                            <ImageView android:id="@+id/home_today_iv_2"
                                android:layout_width="0dp"
                                android:layout_height="20dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_onestar_d"
                                />
                            <ImageView android:id="@+id/home_today_iv_3"
                                android:layout_width="0dp"
                                android:layout_height="20dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_onestar_d"
                                />
                            <ImageView android:id="@+id/home_today_iv_4"
                                android:layout_width="0dp"
                                android:layout_height="20dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_onestar_d"
                                />
                            <ImageView android:id="@+id/home_today_iv_5"
                                android:layout_width="0dp"
                                android:layout_height="20dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_onestar_d"
                                />
                            <ImageView android:id="@+id/home_today_iv_6"
                                android:layout_width="0dp"
                                android:layout_height="20dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_onestar_d"
                                />
                            <ImageView android:id="@+id/home_today_iv_7"
                                android:layout_width="0dp"
                                android:layout_height="20dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_onestar_d"
                                />
                            <ImageView android:id="@+id/home_today_iv_8"
                                android:layout_width="0dp"
                                android:layout_height="20dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_onestar_d"
                                />
                            <ImageView android:id="@+id/home_today_iv_9"
                                android:layout_width="0dp"
                                android:layout_height="20dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_onestar_d"
                                />
                            <ImageView android:id="@+id/home_today_iv_10"
                                android:layout_width="0dp"
                                android:layout_height="20dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_onestar_d"
                                />
                        </LinearLayout>
                        <!-- Yesterday stars -->
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/white"
                            android:layout_weight="1"
                            android:layout_marginTop="8dp"
                            android:weightSum="10"
                            android:orientation="horizontal">
                            <ImageView android:id="@+id/home_yest_iv_1"
                                android:layout_width="0dp"
                                android:layout_height="20dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_onestar_d"
                                />
                            <ImageView android:id="@+id/home_yest_iv_2"
                                android:layout_width="0dp"
                                android:layout_height="20dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_onestar_d"
                                />
                            <ImageView android:id="@+id/home_yest_iv_3"
                                android:layout_width="0dp"
                                android:layout_height="20dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_onestar_d"
                                />
                            <ImageView android:id="@+id/home_yest_iv_4"
                                android:layout_width="0dp"
                                android:layout_height="20dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_onestar_d"
                                />
                            <ImageView android:id="@+id/home_yest_iv_5"
                                android:layout_width="0dp"
                                android:layout_height="20dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_onestar_d"
                                />
                            <ImageView android:id="@+id/home_yest_iv_6"
                                android:layout_width="0dp"
                                android:layout_height="20dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_onestar_d"
                                />
                            <ImageView android:id="@+id/home_yest_iv_7"
                                android:layout_width="0dp"
                                android:layout_height="20dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_onestar_d"
                                />
                            <ImageView android:id="@+id/home_yest_iv_8"
                                android:layout_width="0dp"
                                android:layout_height="20dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_onestar_d"
                                />
                            <ImageView android:id="@+id/home_yest_iv_9"
                                android:layout_width="0dp"
                                android:layout_height="20dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_onestar_d"
                                />
                            <ImageView android:id="@+id/home_yest_iv_10"
                                android:layout_width="0dp"
                                android:layout_height="20dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_onestar_d"
                                />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <TextView
                    android:layout_below="@id/home_linear_content"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/darkGray"
                    />
            </RelativeLayout>
            <!-- End Summary -->
            <!-- Start Protein-->
            <RelativeLayout
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/home_txt_protein"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    style="@style/SectionTitle"
                    android:text="@string/mt_protein"/>
                <TextView
